
config frp 'common'
	option log_max_days '3'
	option login_fail_exit '0'
	option enable_cpool '0'
	option time '40'
	option tcp_mux '1'
	option server_port '7000'
	option log_level 'info'
	option enable_http_proxy '0'
	option protocol 'tcp'
	option server_addr 'frp.freefrp.net'
	option token 'freefrp.net'
	option vhost_http_port '32380'
	option vhost_https_port '32443'
	option enabled '1'

config proxy
	option enable '1'
	option type 'http'
	option domain_type 'subdomain'
	option subdomain 'x86kvmlede'
	option local_ip '10.3.1.3'
	option local_port '80'
	option proxy_protocol_version 'disable'
	option use_encryption '1'
	option use_compression '1'
	option remark 'x86-kvm-lede'

config proxy
	option enable '1'
	option type 'tcp'
	option remote_port '32322'
	option local_ip '10.3.1.3'
	option local_port '22'
	option proxy_protocol_version 'disable'
	option use_encryption '1'
	option use_compression '1'
	option remark 'x86-kvm-ssh'

config proxy
	option enable '1'
	option type 'http'
	option domain_type 'subdomain'
	option subdomain 'x86ikuai'
	option local_ip '10.3.1.1'
	option local_port '80'
	option proxy_protocol_version 'disable'
	option use_encryption '1'
	option use_compression '1'
	option remark 'ikuai-x86'
